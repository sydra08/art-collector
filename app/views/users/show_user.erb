<div id="navbar">
  <a href="/artists" class="button">Browse Artists</a>
  <a href="/artworks" class="button">Browse Artworks</a>
  <a href="/collections/new" class="button">New Collection</a>
  <a href="/logout" class="button">Log Out</a>
</div>

<h1>Welcome <%=current_user.username%>!</h1>
<ul>
  <% current_user.collections.all.each do |c| %>
    <li>
      <strong>
        <a href="/collections/<%=c.id%>"><%=c.name%></a>
      </strong>
      <small>
        <a href="/collections/<%=c.id%>/edit">edit</a>
        <a href="/collections/<%=c.id%>/delete">delete</a>
      </small>
    </li>
  <%end%>
</ul>
